{
  "name": "io-game-template",
  "version": "1.0.0",
  "scripts": {
    "dev:server": "nodemon src/server/index.ts -r dotenv/config",
    "dev:client": "vite build src/client --watch",
    "dev": "concurrently --kill-others \"pnpm dev:client\" \"pnpm dev:server\"",
    "copy:public": "copyfiles -u 3 ./src/client/public/**/* ./dist/public",
    "build": "vite build src/client && tsc -p ./src/server && pnpm copy:public",
    "start": "node -r dotenv/config dist/index.js"
  },
  "dependencies": {
    "express": "^4.19.2",
    "phaser": "^3.80.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.9"
  }
}